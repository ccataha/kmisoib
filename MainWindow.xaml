<Window x:Class="KMiSOIB_lab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KMiSOIB_lab"
        mc:Ignorable="d"
        Title="КМиСОИБ - Лабораторные работы" Height="298.5" Width="200">
    <Grid>
        <Button x:Name="lb1_Button" Content="Лабораторная работа №1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="168" Height="30" Click="Lb1_Button_Click"/>
        <Button x:Name="lb2_Button" Content="Лабораторная работа №2" HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top" Width="168" Height="30" Click="Lb2_Button_Click"/>
        <Button x:Name="lb3_Button" Content="Лабораторная работа №3" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Width="168" Height="30" Click="Lb3_Button_Click"/>
        <Button x:Name="lb4_Button" Content="Лабораторная работа №4" HorizontalAlignment="Left" Margin="10,115,0,0" VerticalAlignment="Top" Width="168" Height="30" Click="Lb4_Button_Click"/>
        <Button x:Name="exit_Button" Content="Exit" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" Width="168" Height="30" Click="Exit_Button_Click"/>

    </Grid>
</Window>
